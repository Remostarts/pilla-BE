        "cookies": "^0.9.1",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "fs-extra": "^11.2.0",
        "handlebars": "^4.7.8",
        "helmet": "^7.1.0",
        "http-status": "^1.7.4",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.9.14",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
