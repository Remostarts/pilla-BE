{
    "name": "pro_cleaner-server",
    "version": "1.0.0",
    "description": "cleaning service provider server",
    "main": "src/server.ts",
    "author": "webreality",
    "license": "ISC",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --env-file=.env src/server.ts ",
        "build": "tsc",
        "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts . ",
        "prettier:check": "prettier --ignore-path .gitignore --check .",
        "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
        "prettier:fix": "prettier --write .",
        "prod": "NODE_ENV=production nodemon server.js",
        "postinstall": "prisma generate",
        "prepare": "husky install",
        "test": "jest"
    },
    "lint-staged": {
        "*.{ts,js}": [
            "npm run lint:fix",
            "npm run prettier:fix",
            "npm run lint:check",
            "npm run prettier:check"
        ]
    },
    "dependencies": {
        "@colors/colors": "^1.6.0",
        "@prisma/client": "^5.17.0",
        "axios": "^1.7.5",
        "bcrypt": "^5.1.1",
