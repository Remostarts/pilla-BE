{
    "name": "pro_cleaner-server",
    "version": "1.0.0",
    "description": "cleaning service provider server",
    "main": "src/server.ts",
    "author": "webreality",
    "license": "ISC",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --env-file=.env src/server.ts ",
        "build": "tsc",
        "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts . ",
        "prettier:check": "prettier --ignore-path .gitignore --check .",
        "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
        "prettier:fix": "prettier --write .",
        "prod": "NODE_ENV=production nodemon server.js",
        "postinstall": "prisma generate",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{ts,js}": [
            "npm run lint:fix",
            "npm run prettier:fix",
            "npm run lint:check",
            "npm run prettier:check"
        ]
    },
    "dependencies": {
        "@colors/colors": "^1.6.0",
        "@prisma/client": "^5.7.0",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "http-status": "^1.7.3",
        "jsonwebtoken": "^9.0.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1",
        "yamljs": "^0.3.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.1",
        "@commitlint/config-conventional": "^18.6.3",
        "@tsconfig/node16": "^16.1.1",
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.4",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/yamljs": "^0.2.34",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "eslint": "^8.55.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-pretty": "^6.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.0.0",
        "prisma": "^5.7.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}
